{
  "name": "remix-vite-mui-issu",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "start": "remix-serve ./build/server/index.js",
    "start:test": "dotenv -e .env.test remix vite:dev --mode test",
    "dev": "remix vite:dev",
    "dev:test": "dotenv -e .env.test remix vite:dev --mode test",
    "build": "remix vite:build",
    "build:test": "dotenv -e .env.test remix vite:build",
    "routes": "remix routes",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "tsc",
    "studio": "prisma studio",
    "migrate-db": "prisma migrate dev",
    "migrate-db:test": "dotenv -e .env.test -- prisma migrate dev",
    "reset-db": "prisma migrate reset --force",
    "reset-db:test": "dotenv -e .env.test -- prisma migrate reset --force",
    "seed-db": "prisma db seed",
    "seed-db:test": "dotenv -e .env.test -- prisma db seed"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^41.0.0",
    "@ckeditor/ckeditor5-react": "^6.2.0",
    "@emotion/cache": "latest",
    "@emotion/react": "latest",
    "@emotion/server": "latest",
    "@emotion/styled": "latest",
    "@mui/lab": "latest",
    "@mui/material": "latest",
    "@mui/x-data-grid": "latest",
    "@mui/x-date-pickers": "latest",
    "@node-saml/passport-saml": "^4.0.4",
    "@prisma/client": "^5.7.1",
    "@remix-run/express": "2.7.2",
    "@remix-run/node": "2.7.2",
    "@remix-run/react": "2.7.2",
    "@remix-run/serve": "2.7.2",
    "@remix-run/server-runtime": "2.7.2",
    "@textea/json-viewer": "^3.1.1",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/luxon": "^3.3.8",
    "cheerio": "^1.0.0-rc.12",
    "clsx": "^2.1.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dd-trace": "^4.21.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fuse.js": "^7.0.0",
    "isbot": "^3.6.8",
    "jsonwebtoken": "^9.0.2",
    "lodash-es": "^4.17.21",
    "luxon": "^3.4.4",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sanitize-html": "^2.7.0",
    "short-unique-id": "^5.0.3",
    "source-map-support": "^0.5.21",
    "tailwind-merge": "^2.2.0",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@remix-run/dev": "2.7.2",
    "@types/ckeditor__ckeditor5-build-classic": "^29.0.1",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/lodash-es": "^4.17.12",
    "@types/morgan": "^1.9.4",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/sanitize-html": "^2.6.2",
    "@types/source-map-support": "^0.5.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "autoprefixer": "^10.4.17",
